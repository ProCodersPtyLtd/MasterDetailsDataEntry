<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Platz.ObjectBuilder</id>
    <version>0.4.0</version>
    <authors>Pro Coders Australia</authors>
    <owners>Pro Coders Australia</owners>
    <projectUrl>https://github.com/ProCodersPtyLtd/MasterDetailsDataEntry</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Platz.ObjectBuilder is a Blazor UI component that allows to build Models and Queries visually and generate C# code based on saved json data.</description>
    <copyright>Copyright (c) 2020 Pro Coders PTY LTD</copyright>
    <tags>platz blazor model visual query component framework object builder</tags>
    <license type="expression">MIT</license>
    <dependencies>
		<group targetFramework="net5.0">
      <dependency id="Platz.SqlForms" version="0.4.0" />
			<dependency id="Microsoft.AspNetCore.Components" version="5.0.3" />
			<dependency id="Microsoft.AspNetCore.Components.Web" version="5.0.3" />
      <dependency id="Microsoft.EntityFrameworkCore" version="5.0.3" />
			<dependency id="Microsoft.EntityFrameworkCore.Relational" version="5.0.3" />
			<dependency id="System.ComponentModel.Annotations" version="5.0.0" />
			<dependency id="System.Text.Json" version="5.0.1" />
		</group>
    </dependencies>
    <contentFiles>
      <files include="**\*.json" buildAction="None" />
      <files include="**\*.cmd" buildAction="None" />
      <files include="**\*.txt" buildAction="None" />
      <files include="**\*.sql" buildAction="None" />
      <files include="**\*.cs" buildAction="None" />
      <files include="**\*.razor" buildAction="None" />
      <files include="**\*.tt" buildAction="None" />
      <files include="**\*.ttinclude" buildAction="None" />
    </contentFiles>
  </metadata>
  <files>
    <file src="readme.txt" />
    <file src="..\wwwroot\background.png" target="staticwebassets\background.png" />
    <file src="..\wwwroot\objectBuilder.js" target="staticwebassets\objectBuilder.js" />
    <file src="..\obj\Release\net5.0\scopedcss\projectbundle\Platz.ObjectBuilder.Blazor.bundle.scp.css" target="staticwebassets\Platz.ObjectBuilder.Blazor.bundle.scp.css" />

    <file src="build\Microsoft.AspNetCore.StaticWebAssets.props" target="build\Microsoft.AspNetCore.StaticWebAssets.props" />
    <file src="build\Platz.ObjectBuilder.props" target="build\Platz.ObjectBuilder.props" />
    <file src="build\buildTransitive.Platz.ObjectBuilder.Blazor.props" target="buildTransitive\Platz.ObjectBuilder.Blazor.props" />
    <file src="build\buildMultiTargeting.Platz.ObjectBuilder.Blazor.props" target="buildMultiTargeting\Platz.ObjectBuilder.Blazor.props" />

    <file src="..\bin\Release\net5.0\Platz.ObjectBuilder.Blazor.dll" target="lib\net5.0\Platz.ObjectBuilder.Blazor.dll" />
    <file src="..\bin\Release\net5.0\Platz.ObjectBuilder.dll" target="lib\net5.0\Platz.ObjectBuilder.dll" />
    
  </files>
</package>