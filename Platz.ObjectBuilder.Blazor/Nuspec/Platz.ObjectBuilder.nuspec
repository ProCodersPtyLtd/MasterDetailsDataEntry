<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Platz.ObjectBuilder</id>
    <version>0.1.4</version>
    <authors>Pro Coders Australia</authors>
    <owners>Pro Coders Australia</owners>
    <projectUrl>https://github.com/ProCodersPtyLtd/MasterDetailsDataEntry</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Platz.ObjectBuilder is a Blazor UI component that allows to build Models and Queries visually and generate C# code based on saved json data.</description>
    <copyright>Copyright (c) 2020 Pro Coders PTY LTD</copyright>
    <tags>platz blazor model visual query component framework object builder</tags>
    <license type="expression">MIT</license>
    <dependencies>
		<group targetFramework="netstandard2.1">
			<dependency id="Microsoft.AspNetCore.Components" version="3.1.9" />
			<dependency id="Microsoft.AspNetCore.Components.Web" version="3.1.9" />
      <dependency id="Microsoft.EntityFrameworkCore" version="3.1.9" />
			<dependency id="Microsoft.EntityFrameworkCore.Relational" version="3.1.9" />
			<dependency id="System.ComponentModel.Annotations" version="5.0.0" />
			<dependency id="System.Text.Json" version="5.0.0" />
			<dependency id="blazor-dragdrop" version="2.2.0" />
		</group>
    </dependencies>
    <contentFiles>
      <files include="**\*.json" buildAction="None" />
      <files include="**\*.cmd" buildAction="None" />
      <files include="**\*.txt" buildAction="None" />
      <files include="**\*.sql" buildAction="None" />
      <files include="**\*.cs" buildAction="None" />
      <files include="**\*.razor" buildAction="None" />
      <files include="**\*.tt" buildAction="None" />
      <files include="**\*.ttinclude" buildAction="None" />
    </contentFiles>
  </metadata>
  <files>
    <file src="readme.txt" />
    <file src="contentFiles\any\any\Platz.Config.Link\*" target="contentFiles\any\any\Platz.Config.Link"/>
    <file src="contentFiles\any\any\Platz.Example.Link\*" target="contentFiles\any\any\Platz.Example.Link"/>
    <file src="..\bin\Release\netstandard2.1\Platz.ObjectBuilder.Blazor.dll" target="lib\netstandard2.1\Platz.ObjectBuilder.Blazor.dll" />
    <file src="..\bin\Release\netstandard2.1\Platz.ObjectBuilder.dll" target="lib\netstandard2.1\Platz.ObjectBuilder.dll" />
    <file src="..\..\Platz.ObjectBuilder.Templates\bin\Release\Platz.ObjectBuilder.Templates.dll" target="lib\Platz.ObjectBuilder.Templates.dll" />
    <file src="build\Platz.ObjectBuilder.props" target="build\Platz.ObjectBuilder.props" />
    <file src="tools\Platz.ObjectBuilder.Templates.dll" target="tools\Platz.ObjectBuilder.Templates.dll" />
  </files>
</package>